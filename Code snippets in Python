#Assigning elements to different list

names=['Raghav','Priya','Dharan','Samuel','Karthik']
num1=[1,27,89,99,121]

#Accessing element from a tuple

tup1=(77,89,100,67,29)
tup2=('Apple','Orange','Pineapple','Lemon','Peach')
print("The element in tup1 at index 2 is:")
print (tup1[0])
print("The elements in tup2 in the range 1:3 is")
print(tup2[1:3])

#Deleting different dictionary elements

marks={"Akshay":66,"Kaushik":75,"Mark":83,"Sony":67}
del marks["Mark"]
print(marks)


